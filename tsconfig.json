{
  "include": [
    "src", "**/*.ts", "**/*.js"
  ],
  "compilerOptions": {
    "target": "es6",
    "lib": [
        "es6"
    ],
    "module": "esnext",
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node"
  }
}
